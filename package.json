{
  "name": "kiro-constellation",
  "displayName": "kiro-constellation",
  "description": "The stellar devloper toolkit for Kiro IDE",
  "version": "0.0.1",
  "type": "module",
  "publisher": "kiro-dev",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.cjs",
  "contributes": {
    "commands": [
      {
        "command": "kiro-constellation.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "constellation.testLlmConnection",
        "title": "Test LLM Connection"
      },
      {
        "command": "constellation.testDocstringGeneration",
        "title": "Test Docstring Generation"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "vite build",
    "watch": "vite build --watch",
    "package": "vite build --mode production",
    "lint": "eslint src",
    "compile-tests": "tsc -p tsconfig.test.json",
    "pretest": "npm run compile-tests",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.7",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "glob": "^11.0.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.6"
  },
  "dependencies": {
    "@babel/parser": "^7.28.0",
    "@babel/traverse": "^7.28.0",
    "dotenv": "^17.2.1"
  }
}