{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "ES2022",
    "outDir": "out",
    "lib": [
      "ES2022"
    ],
    "sourceMap": true,
    "rootDir": "src",
    "strict": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "vite.config.ts",
    "vite.config.js",
    "*.config.ts",
    "*.config.js"
  ]
}