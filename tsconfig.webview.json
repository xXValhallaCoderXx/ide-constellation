{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist/src/ui",
    "rootDir": "./src/ui",
    "declaration": false,
    "sourceMap": false
  },
  "include": ["src/ui/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
